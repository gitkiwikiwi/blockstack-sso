{
	"name": "build",
	"version": "1.0.0",
	"description": "This broject is for building the client side files.",
	"main": " ",
	"scripts": {
		"build": "browserify build.js -o blockstack.js",
		"translate": "babel blockstack.js --out-file blockstack.es5.js",
		"compress": "uglifyjs blockstack.es5.js -c -m -o blockstack.min.js",
		"clean": "rm blockstack.js blockstack.es5.js",
		"all": "npm run build && npm run translate && npm run compress && npm run clean"
	},
	"author": "Saul Boyd",
	"license": "ISC",
    "devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"browserify": "^14.1.0",
	    "uglify-js": "^3.3.20"
	},
	"dependencies": {
		"blockstack": "^0.15.0"
	}
}
